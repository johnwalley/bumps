{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "max-len": 0,
    "no-undef": 0,
    "quote-props": [
      "error",
      "consistent-as-needed"
    ],
    "comma-dangle": 0
  }
}